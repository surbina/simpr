{
  "manifest_version": 2,
  "name": "SimPR",
  "description": "SimPR is an extension that helps to simplify reviewing a PR",
  "version": "0.1.0",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "SimPR",
    "default_popup": "popup.html"
  }
}
